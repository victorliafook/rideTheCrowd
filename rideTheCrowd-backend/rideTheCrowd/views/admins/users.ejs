<div class="container-fluid">
  <div class="row">
    <div class="col-lg-12">
      <h2>Users Registered</h2>
      <div class="table-responsive">
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Id</th>
              <th>Name</th>
              <th>Email</th>
              <th>Country</th>
              <th>City</th>
              <th>Created</th>
            </tr>

          </thead>
          <tbody>
            <% _.each(users, function(u){ %>
              <tr>
                <td>
                  <%= u.id %>
                </td>
                <td>
                  <%= u.firstname + ' ' + u.surname %>
                </td>
                <td>
                  <%= u.email %>
                </td>
                <td>
                  <%= u.country %>
                </td>
                <td>
                  <%= u.city %>
                </td>
                <td>
                  <%= sails.moment(u.createdAt).format('MM-DD-YYYY h:mm:ss a') %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
